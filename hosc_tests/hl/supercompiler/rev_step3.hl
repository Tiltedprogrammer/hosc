
list $a :: Nil | Cons $a (list $a);

          case  ((f y) u)  of {
            Cons y1 w :
              (Cons
                y1
                  ((g w) v1));
            Nil : (Cons v1 Nil);
          }
          
          where
  
f = %u1 {
    %v1 {
      case  u1  of {
        Nil : (Cons v1 Nil);
        Cons u y :
          case  ((f y) u)  of {
            Cons y1 w :
              (Cons
                y1
                  ((g w) v1));
            Nil : (Cons v1 Nil);
          };
      }
    }
  }
  
g = %p1 {
                    %t1 {
                      case  p1  of { Nil : (Cons t1 Nil); Cons x1 v : (Cons x1 ((g v) t1)); }
                    }
                  }