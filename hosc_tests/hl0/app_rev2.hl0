list $a :: Nil | Cons $a (list $a);

case rev xs2 (Cons x2 Nil)

of {
	Nil : ys;
	Cons x1 xs1 : Cons x1 (app xs1 ys);
}


where

rev = %xs { %ys {
  case xs of {
    Nil : ys;
    Cons x1  xs1 :  rev xs1 (Cons x1 ys);
  }
}}

app = %xs {
  %ys {
    case xs of {
      Nil : ys;
      Cons z zs : Cons z (app zs ys);
    }
  }
}