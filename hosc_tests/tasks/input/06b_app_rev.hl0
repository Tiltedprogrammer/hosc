list $a :: Nil | Cons $a (list $a);

listId (app Nil xs)

where

rev = %xs { %ys{
  case xs of {
    Nil : ys;
    Cons x1 xs1 : rev xs1 (Cons x1 ys);
  }
}}

app = %xs {
  %ys {
    case xs of {
      Nil : ys;
      Cons z zs : Cons z (app zs ys);
    }
  }
}

listId = %x {
	case x of {
		Nil: Nil;
		Cons x1 xs1: Cons x1 (listId  xs1);
	}

}