list $a :: Nil | Cons $a (list $a);

join Nil (map (map a) xs)

where

map = %f {
  %list {
    case list of {
      Nil : Nil;
      Cons x xs : Cons (f x) (map f xs);
    }
  } 
}

compose = %f {%g { %x { f (g x)}}}

unit = %x {Cons x Nil}

join = %x { %xs {
	case xs of {
		Nil: x;
		Cons x1 xs1: case x of {Nil: join x1 xs1; Cons x2 xs2 : Cons x2 (join xs2 xs);};
	}
}}