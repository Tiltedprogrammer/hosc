list $a :: Nil  | Cons $a (list $a);
number  :: Z  | S number;
boolean  :: True  | False ;
case  S x1  of {
  Z : True;
  S s :
    ( letrec
      f = %u2 {
        %v2 {
          case  u2  of {
            Z :
              ( letrec
                g = %w2 {
                  %p2 {
                    case  w2  of { Z : True; S v : case  p2  of { Z : False; S p : ((g v) p); }; }
                  }
                }
              in 
                ((g s) v2));
            S u1 : ((f u1) (S v2));
          }
        }
      }
    in 
      ((f s) y));
}