data List a = Nil | Cons a (List a);

case  (rev v3)  of { Cons v9 v10 -> (Cons v9 ((app v10) (Cons v2 Nil))); Nil  -> (Cons v2 Nil); }

where

rev = \xs -> 
  case xs of {
    Nil -> Nil;
    Cons z  zs -> app (rev zs) (Cons z Nil);
  };

app = \xs ys ->
    case xs of {
      Nil -> ys;
      Cons z zs -> Cons z (app zs ys);
    };